<UserControl x:Class="Proiect_Algoritmica.Views.Node"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Proiect_Algoritmica.Views"
             mc:Ignorable="d"
             d:DesignHeight="50" d:DesignWidth="50">
    <Grid ZIndex="1" >
        <Ellipse Name="Ellipse" Fill="Cyan" IsHitTestVisible="True"/>
        <Label IsHitTestVisible="False" Name="Label" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Content="1" Foreground="Black"></Label>
    </Grid>
</UserControl>

