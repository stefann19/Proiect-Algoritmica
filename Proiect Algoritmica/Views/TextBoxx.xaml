<UserControl x:Class="Proiect_Algoritmica.Views.TextBoxx"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Proiect_Algoritmica.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
         <TextBox Name="TextBox" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="DarkCyan" FontFamily="Segoe UI"></TextBox>
    </Grid>
</UserControl>
